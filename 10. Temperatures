def fahrenheit_to_celsius(temp):
    celsius = (temp - 32) / 1.8
    print (celsius)
    return celsius


def celsius_to_fahrenheit(temp):
    fahrenheit = temp * 1.8 + 32
    print (fahrenheit)
    return fahrenheit



